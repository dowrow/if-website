{
  "name": "typography.css",
  "version": "0.1.0",
  "description": "CSS Framework for Web Typography",
  "keywords": [
    "css",
    "scss",
    "typography"
  ],
  "homepage": "https://github.com/MunifTanjim/typography.css#readme",
  "bugs": "https://github.com/MunifTanjim/typography.css/issues",
  "license": "MIT",
  "author": "MunifTanjim (https://muniftanjim.com)",
  "files": [
    "dist",
    "src"
  ],
  "main": "dist/css/typography.css",
  "repository": "github:MunifTanjim/typography.css",
  "scripts": {
    "build:css": "npm-run-all -s css:compile css:postcss css:minify --silent",
    "css:compile": "node ./scripts/css-compile.js",
    "css:postcss": "node ./scripts/css-postcss.js",
    "css:minify": "node ./scripts/css-minify.js",
    "prepare": "npm-run-all -s build:css --silent"
  },
  "devDependencies": {
    "autoprefixer": "^8.5.0",
    "clean-css": "^4.1.11",
    "mkdirp": "^0.5.1",
    "modularscale-sass": "^3.0.5",
    "node-sass": "^4.9.0",
    "npm-run-all": "^4.1.3",
    "postcss": "^6.0.22",
    "sass-math-pow": "^0.1.5"
  }
}
